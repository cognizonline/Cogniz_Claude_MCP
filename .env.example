# Cogniz MCP Server - Environment Variables
# Copy this file to .env and fill in your values

# Required: Your Cogniz API key from https://cogniz.online/account
COGNIZ_API_KEY=mp_1_your_api_key_here

# Optional: Cogniz base URL (default: https://cogniz.online)
COGNIZ_BASE_URL=https://cogniz.online

# Optional: Default project ID (default: "default")
COGNIZ_PROJECT_ID=default

# Optional: Server port for remote MCP (default: 3000)
PORT=3000
